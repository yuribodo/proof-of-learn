		"@types/cors": "^2.8.18",
		"@types/express": "^5.0.1",
		"@types/jsonwebtoken": "^9.0.9",
		"@types/node": "^22.15.17",
		"prisma": "^6.7.0",
		"tsx": "^4.19.4",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"@google/generative-ai": "^0.2.1",
		"@prisma/client": "6.7.0",
